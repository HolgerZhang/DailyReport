{
  "VERSION": "4.0.0",
  "__notes": "文件名：general.json，请根据模版创建自己的配置文件",
  "webdriver": {
    "browser": "Chrome",
    "driver_path": "data/driver"
  },
  "user_list": ["stu1", "stu2"],
  "user_config_folder": "configurations",
  "scheduler": {
    "__notes": "时间：字符串(范围：0-23)，单个或多个时间，逗号隔开或用'-'表示区间，如下",
    "hour": "7",
    "minute": "30"
  },
  "mail": {
    "sender": {
      "user": "username@stu.suda.edu.cn",
      "passwd": "1234567890",
      "host": "smtp.stu.suda.edu.cn",
      "port": 465,
      "starttls": false,
      "__notes": "port为smtp服务器地址端口；starttls为true表示开启starttls加密"
    },
    "receivers": [
      "username@stu.suda.edu.cn"
    ]
  }
}